DISTFILES = Makefile.in $(EXAMPLEFILES)

MYSTICDISTFILES = $(DISTFILES)

all: 
	for dir in $(SUBDIRS); do $(MAKE) -C $$dir $@ || exit $$?; done
.PHONY: all

check: all
.PHONY: check

install: all
	$(MKPATH) $(examplesdir)
	$(INSTALL_DATA) $(EXAMPLEFILES) $(examplesdir)
	for dir in $(SUBDIRS); do $(MAKE) -C $$dir $@ || exit $$?; done
.PHONY: install

uninstall:
	$(RM) -rf $(examplesdir)
.PHONY: uninstall

clean:
.PHONY: clean

mostlyclean:
.PHONY: mostlyclean

distclean: clean
	rm -f Makefile
.PHONY: distclean

realclean: distclean
	rm -f tags TAGS
.PHONY: realclean

dist:
	for dir in $(SUBDIRS); do mkdir ../`cat ../.fname`/examples/$$dir; $(MAKE) -C $$dir $@ || exit $$?; done
	ln $(DISTFILES) ../`cat ../.fname`/examples
.PHONY: dist

mystic-dist:
	for dir in $(SUBDIRS); do mkdir ../`cat ../.fname`/examples/$$dir; $(MAKE) -C $$dir $@ || exit $$?; done
	ln $(MYSTICDISTFILES) ../`cat ../.fname`/examples
.PHONY: mystic-dist

Makefile: Makefile.in ../config.status
	cd ..; sh config.status
