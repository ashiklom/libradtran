#
# Makefile for libRadtran's data/ic/yang2013 directory
#
# Adopted from ../shettle/Makefile.in by Claudia Emde

TOPDIR = ../../..

srcdir = @srcdir@
VPATH = $(srcdir)

THISDIR = yang2013

SUBDIRS = 

DATAFILES = $(wildcard ic.*.cdf)

DISTFILES = Makefile.in #$(DATAFILES)

MYSTICDISTFILES = $(DISTFILES)

include $(TOPDIR)/Makeconf

all: 

dist: all
	ln $(DISTFILES) ../../../`cat ../../../.fname`/data/ic/$(THISDIR)
.PHONY: dist

mystic-dist: all
	ln $(MYSTICDISTFILES) ../../../`cat ../../../.fname`/data/ic/$(THISDIR)
.PHONY: mystic-dist

install: all
ifeq (@FOUND_YANG2013@,yes)
	$(MKPATH) $(datafiledir)/ic/yang2013
	$(INSTALL_DATA) $(DATAFILES) $(datafiledir)/ic/$(THISDIR)
	for dir in $(SUBDIRS); do $(MAKE) -C $$dir $@ || exit $$?; done
endif
.PHONY: install

clean:

mostlyclean: clean

distclean: clean
	rm -f Makefile config.status

realclean: distclean

Makefile: Makefile.in ../../../config.status
	cd ../../../; sh config.status

# Prevent GNU make v3 from overflowing arg limit on SysV.
.NOEXPORT:

# eof
