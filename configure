### Do special things for gcc.

ADD_CC_WALL=false
gcc_version=`$CC -v 2>&1 | grep "^.*gcc version" | \
  sed -e 's/^.*g.. version *//' -e 's/cygnus-//'| sed -e 's/egcs-//'`
case "$gcc_version" in
  2.* | 3.* | 4.* | 5.* | 6.* | 7.* | 8.* | 9.*)
    if test -z "$EXTERN_CFLAGS"; then
      ADD_CC_WALL=true
    fi
    if test -z "$LDFLAGS"; then
      LDFLAGS="-g"
      { $as_echo "$as_me:${as_lineno-$LINENO}: result: defining LDFLAGS to be $LDFLAGS" >&5
$as_echo "defining LDFLAGS to be $LDFLAGS" >&6; }
    fi
  ;;
  1.*)
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: gcc version $gcc_version is likely to cause problems" >&5
$as_echo "$as_me: WARNING: gcc version $gcc_version is likely to cause problems" >&2;}
  ;;
  *)
    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: libRadtran has only been tested with gcc, and I can't find it" >&5
$as_echo "$as_me: WARNING: libRadtran has only been tested with gcc, and I can't find it" >&2;}
  ;;
esac

